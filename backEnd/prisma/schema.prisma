generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  USER
  ADMIN
}

model Usuario {
  id           Int    @id @default(autoincrement())
  nomeCompleto String
  email        String @unique
  senha        String
  telefone     String
  cpf          String @unique
  tipoDaConta  Role
}
